scancodes=["a","b","c"]
shiftcodes=["A","B","C"]
altcodes=["1","2","3"]

print("Scancode lookup table")
for i in range(3):
	print("\"" + scancodes[i] + "\":\"\\x" + str("0x%02X" % i)[-2:] + "\",")
print("Shiftcode lookup table")
for i in range(3):
	print("\""+shiftcodes[i]+"\":\""+scancodes[i]+"\",")
print("Altcode lookup table")
for i in range(3):
	print("\""+altcodes[i]+"\":\""+scancodes[i]+"\",")